{
    "env": "CartPole-v0",
    "episodes": 5000,
    "render_episode_interval": 50,
    "max_episode_length": 1000,

    "frame_stack": 1,

    "replay_buffer_size": 10000,
    "training_interval": 4,
    "batch_size": 32,
    "hidden_dim": 32,

    "learning_rate": 0.001,
    "gamma": 0.99,
    "target_update_rate": 0.001,

    "epsilon_min": 0.02,
    "epsilon_max": 1.0,
    "epsilon_decay": 0.999,

    "dueling": false,
    "double": true
}